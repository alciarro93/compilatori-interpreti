//Single class with multiple method:
class Rectangle(int l1, int l2) {
    int area() l1 * l2;
    int getL1() l1;
    int getL2() l2;
    int scaleArea(int scaling) scaling * l1 * l2;
    int calcArea(int l1, int l2) l1 * l2;
  };
  let Rectangle r = new Rectangle(2, 4);
  in print(r.calcArea(3, 5));
//- "15"