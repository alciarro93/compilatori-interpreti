//Call method on object returned by method2:
class Numero (int x) {
        Numero uno() new Numero(2 * x);
        int due() x;
    };
    let
        Numero num = new Numero(2);
        Numero num1 = new Numero(3);
        Numero num2 = num.uno();
        Numero num3 = num1.uno();
    in
        print(num2.due() + num3.due());
//-   "10"